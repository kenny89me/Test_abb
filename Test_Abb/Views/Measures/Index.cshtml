@model IEnumerable<Test_Abb.Models.Measure>
@using Test_Abb.Models;

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MotorId)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MotorId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActualCurrentA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DifferenceA)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ElectricMotor.CurrentA)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MotorId }) |
            @Html.ActionLink("Details", "Details", new { id = item.MotorId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.MotorId })
        </td>
    </tr>
    }

</table>
